<b>Your submissions</b>
<table class="main_table">
  <tr>
    <th style="width:1%"></th>
    <th>Date</th>
    <th>Score</th>
  </tr>

  <% @submissions.each_with_index do |sub, i| %>
    <tr>
      <td> <%=i+1 %> </td>
      <td> <%= link_to sub.created_at.strftime("%b %d %Y, %H:%M"), sub %> </td>
      <td> <%= sub.score %> </td>
    </tr>
  <% end %>
</table>
